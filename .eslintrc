{
    "env": {
        "browser": true,
        "es6": true,
        "jasmine": true
    },
    "globals": {
      "ENV": true
    },
    "plugins": [
      "jasmine"
    ],
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            "error",
            2
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-console": [
          "warn"
        ]
    }
}
